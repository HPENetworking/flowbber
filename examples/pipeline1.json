{
    "schedule": {
        "frequency": "10 seconds",
        "samples": 4
    },
    "sources": [
        {
            "type": "timestamp",
            "id": "timestamp",
            "config": {
                "epochf": true,
                "iso8601": true,
                "strftime": "%Y-%m-%d %H:%M:%S"
            }
        },
        {
            "type": "my_source",
            "id": "my_source",
            "config": {}
        },
        {
            "type": "user",
            "id": "user",
            "config": {}
        },
        {
            "type": "env",
            "id": "env",
            "config": {
                "include": [
                    "LANG"
                ],
                "exclude": [],
                "lowercase": false
            }
        },
        {
            "type": "env",
            "id": "env_lower",
            "config": {
                "include": [
                    "PYTHONHASHSEED"
                ],
                "exclude": [],
                "lowercase": true
            }
        },
        {
            "type": "cobertura",
            "id": "coverage",
            "config": {
                "xmlpath": "{pipeline.dir}/coverage.xml"
            }
        }
    ],
    "aggregators": [],
    "sinks": [
        {
            "type": "print",
            "id": "print",
            "config": {}
        },
        {
            "type": "archive",
            "id": "archive",
            "config": {
                "output": "data.json",
                "override": true,
                "create_parents": true
            }
        },
        {
            "type": "template",
            "id": "template1",
            "config": {
                "template": "file://{pipeline.dir}/template1.tpl",
                "output": "render1.html",
                "override": true,
                "create_parents": true
            }
        },
        {
            "type": "template",
            "id": "template2",
            "config": {
                "template": "python://flowconf:atemplate",
                "output": "render2.html",
                "override": true,
                "create_parents": true
            }
        },
        {
            "type": "mongodb",
            "id": "mongodb",
            "config": {
                "uri": "mongodb://localhost:27017/",
                "database": "flowbber",
                "collection": "pipeline1data"
            }
        },
        {
            "type": "influxdb",
            "id": "influxdb",
            "config": {
                "uri": "influxdb://localhost:8086/",
                "database": "flowbber"
            }
        }
    ]
}
