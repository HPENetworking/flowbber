{
    "sources": [
        {
            "type": "timestamp",
            "key": "timestamp",
            "config": {
                "epochf": true,
                "iso8601": true,
                "strftime": "%Y-%m-%d %H:%M:%S"
            }
        },
        {
            "type": "user",
            "key": "user",
            "config": {}
        }
    ],
    "aggregators": [],
    "sinks": [
        {
            "type": "print",
            "config": {}
        },
        {
            "type": "archive",
            "config": {
                "output": "data.json",
                "create_parents": true,
                "override": true
            }
        },
        {
            "type": "template",
            "config": {
                "template": "template1.tpl",
                "output": "render1.html",
                "create_parents": true,
                "override": true
            }
        },
        {
            "type": "mongo",
            "config": {
                "uri": "mongodb://localhost:27017/",
                "database": "flowbber",
                "collection": "pipeline1data"
            }
        },
        {
            "type": "influx",
            "config": {
                "uri": "influxdb://localhost:8086/",
                "database": "flowbber"
            }
        }
    ]
}
